                    transform: translateY(-26px);
                    opacity: 0;
                }
                35%, 80% {
                    transform: translateY(0px);
                    opacity: 1;
                }
                92%, 100% {
                    transform: translateY(-4px);
                    opacity: 0;
                }
            }
            @keyframes animLet {
                0%, 25% {
                    transform: translateY(-26px);
                    opacity: 0;
                }
                45%, 80% {
                    transform: translateY(0px);
                    opacity: 1;
                }
                92%, 100% {
                    transform: translateY(-4px);
                    opacity: 0;
                }
            }
            @keyframes animGo {
                0%, 35% {
                    transform: translateY(-26px);
                    opacity: 0;
                }
                55%, 80% {
                    transform: translateY(0px);
                    opacity: 1;
                }
                92%, 100% {
                    transform: translateY(-4px);
                    opacity: 0;
                }
